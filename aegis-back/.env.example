# AEGIS RPA Backend Configuration
# Copy this file to .env and fill in your values

# ADK Configuration
GOOGLE_ADK_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-robotics-er-1.5-preview
ADK_TIMEOUT=30

# Server Configuration
HOST=0.0.0.0
PORT=8000
LOG_LEVEL=INFO

# Storage Configuration
HISTORY_DIR=./data/history
CACHE_DIR=./data/cache
MAX_CACHE_SIZE=100

# Performance Configuration
MAX_CONCURRENT_SESSIONS=1
REQUEST_QUEUE_SIZE=10
WEBSOCKET_PING_INTERVAL=30

# Local App Launcher Configuration
LOCAL_LAUNCHER_ENABLED=true
LOCAL_LAUNCHER_MAPPING_FILE=config/app_mappings.json
LOCAL_LAUNCHER_MENU_DELAY=1.0
LOCAL_LAUNCHER_TYPING_INTERVAL=0.1
LOCAL_LAUNCHER_SEARCH_DELAY=1.0
LOCAL_LAUNCHER_LAUNCH_DELAY=2.0
LOCAL_LAUNCHER_VERIFICATION_TIMEOUT=5.0
